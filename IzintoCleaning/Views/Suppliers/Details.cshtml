@model IzintoCleaning.Models.Supplier

@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>

<div>
    <h4>Supplier</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
           Added By
        </dt>

        <dd>
            @Html.DisplayFor(model => model.admin.AdminName)
        </dd>

        <dt>
           Name
        </dt>

        <dd>
            @Html.DisplayFor(model => model.SuppName)
        </dd>

        <dt>
            Email
        </dt>

        <dd>
            @Html.DisplayFor(model => model.SuppEmail)
        </dd>

        <dt>
           Phone Number
        </dt>

        <dd>
            @Html.DisplayFor(model => model.SuppPhone)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Edit", "Edit", new { id = Model.SuppID }) |
    @Html.ActionLink("Back to List", "Index")
</p>
